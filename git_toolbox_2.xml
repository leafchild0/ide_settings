<application>
  <component name="GitToolBoxAppSettings2">
    <option name="absoluteDateTimeStyle" value="MDY_TIME" />
    <option name="autoFetchIntervalMinutes" value="30" />
    <option name="commitMessageValidationConfig">
      <CommitMessageValidationConfig>
        <option name="regexConfig">
          <RegexConfig>
            <option name="expression" value="(?:fix|chore|feat)(?:\(.*\))?: [A-Z].*\s#\d+" />
            <option name="testInput" value="fix: Bug #123" />
          </RegexConfig>
        </option>
      </CommitMessageValidationConfig>
    </option>
    <option name="outdatedBranchesAutoCleanup">
      <OutdatedBranchesAutoCleanupConfig>
        <option name="autoCheckEnabled" value="true" />
      </OutdatedBranchesAutoCleanupConfig>
    </option>
    <option name="outdatedBranchesExclusionGlobs">
      <list>
        <option value="master" />
        <option value="main" />
        <option value="develop" />
        <option value="release*" />
      </list>
    </option>
    <option name="previousVersionMigrated" value="true" />
    <option name="version" value="6" />
  </component>
</application>